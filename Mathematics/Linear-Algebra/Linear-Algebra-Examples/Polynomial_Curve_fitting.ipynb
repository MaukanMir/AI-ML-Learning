   "source": [
    "import numpy as np\n",
    "\n",
    "x = [0, 1, 2, 3, 4, 5]\n",
    "y = [1, 2, 1.5, 8, 1.6, 1.5]\n",
    "\n",
    "y_diff = np.diff(y)\n",
    "x_diff = np.diff(x)\n",
    "slope = y_diff / x_diff\n",
    "\n",
    "if np.max(np.abs(slope)) > 3:\n",
    "    print(\"Possible discontinuity detected\")"
   ]
